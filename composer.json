{
	"name": "kuriousagency/email-subscriptions",
	"description":
		"Allows subscribing and unsubscribing from 3rd party email lists.",
	"type": "craft-plugin",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"email subscriptions"
	],
	"support": {
		"docs":
			"https://github.com/KuriousAgency/email-subscriptions/blob/master/README.md",
		"issues": "https://github.com/KuriousAgency/email-subscriptions/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\emailsubscriptions\\": "src/"
		}
	},
	"extra": {
		"name": "Email Subscriptions",
		"handle": "email-subscriptions",
		"hasCpSettings": true,
		"hasCpSection": false,
		"changelogUrl":
			"https://raw.githubusercontent.com/KuriousAgency/email-subscriptions/master/CHANGELOG.md",
		"components": {
			"emailSubscriptionsService":
				"kuriousagency\\emailsubscriptions\\services\\EmailSubscriptionsService"
		},
		"class": "kuriousagency\\emailsubscriptions\\EmailSubscriptions"
	}
}
